<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.clubin.com">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CLUBIN">
        <activity
            android:name="com.clubin.com.ui.auth.signup.SignUpActivity"
            android:exported="false"
            android:label="@string/title_activity_sign_up"
            android:theme="@style/Theme.CLUBIN.NoActionBar" />
        <activity
            android:name="com.clubin.com.ui.fragment.profile.AccountEditActivity"
            android:exported="false"
            android:theme="@style/Theme.CLUBIN.NoActionBar" />
        <activity
            android:name="com.clubin.com.ui.auth.login.view.LoginActivity"
            android:exported="false"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.CLUBIN.NoActionBar" />
        <activity
            android:name="com.clubin.com.ui.auth.onboarding.view.OnboardingActivity"
            android:exported="false"
            android:theme="@style/Theme.CLUBIN.NoActionBar" />

        <activity
            android:name="com.clubin.com.ui.auth.passwordreset.view.PasswordResetNewPasswordActivity"
            android:exported="false"
            android:theme="@style/Theme.CLUBIN.NoActionBar" />
        <activity
            android:name="com.clubin.com.ui.auth.passwordreset.view.PasswordResetCodeActivity"
            android:exported="false"
            android:theme="@style/Theme.CLUBIN.NoActionBar" />
        <activity
            android:name="com.clubin.com.ui.auth.passwordreset.view.PasswordResetEmailActivity"
            android:exported="false"
            android:theme="@style/Theme.CLUBIN.NoActionBar" />
        <activity
            android:name="com.clubin.com.WelcomeActivity"
            android:exported="true"
            android:theme="@style/Theme.CLUBIN.NoActionBar">
         <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.clubin.com.ui.tabbar.TabBarActivity"
            android:exported="true"
            android:theme="@style/Theme.CLUBIN.NoActionBar">

          <!--  <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>-->
        </activity>
        <activity
            android:name="com.clubin.com.ui.themeparty.view.ThemePartyActivity"
            android:exported="false"
            android:theme="@style/Theme.CLUBIN.NoActionBar" />
    </application>

</manifest>